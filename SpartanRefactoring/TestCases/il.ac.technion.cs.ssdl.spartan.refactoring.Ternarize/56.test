package p;

public class Blah {
	public void updateCurrentProgress(final int current, final int target) {
	    if (target == 0) {
	      progressBarCurrent.setString("0/0");
	      progressBarCurrent.setValue(0);
	    } else {
	      progressBarCurrent.setString(current + "/" + target);
	      progressBarCurrent.setValue(current * 100 / target);
	    }
	  }
}